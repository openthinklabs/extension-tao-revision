{
  "name": "openthinklabs/extension-tao-revision",
  "description": "The extension allows saving the intermediate state of objects and restoring them",
  "type": "tao-extension",
  "authors": [
    {
      "name": "Open Assessment Technologies SA"
    }
  ],
  "keywords": [
    "tao",
    "computer-based-assessment"
  ],
  "homepage": "http://www.taotesting.com",
  "license": [
    "GPL-2.0-only"
  ],
  "extra": {
    "tao-extension-name": "taoRevision"
  },
  "minimum-stability": "dev",
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/oatbox-extension-installer.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/generis.git"
		},        
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/tao-core.git"
		},                
		{
		    "type": "vcs",
		    "url": "git@github.com:openthinklabs/extension-tao-item.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/extension-tao-mediamanager.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/extension-tao-itemqti.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/extension-tao-test.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/lib-generis-search.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/jig.git"
		},
		{
			"type": "vcs",
			"url": "git@github.com:openthinklabs/composer-npm-bridge.git"
		}            
	],		
  "require": {
    "openthinklabs/oatbox-extension-installer": "~1.1||dev-master",
    "openthinklabs/generis" : "15.13.1.4",
    "openthinklabs/tao-core" : "48.75.0.6",
    "openthinklabs/extension-tao-item" : ">=11.8.0",
    "openthinklabs/extension-tao-mediamanager" : ">=12.9.0",
    "openthinklabs/extension-tao-itemqti" : ">=28.8.0",
    "openthinklabs/extension-tao-test" : ">=15.0.0"
  },
  "autoload": {
    "psr-4": {
      "oat\\taoRevision\\": ""
    }
  },
  "config": {
    "allow-plugins": {
      "oat-sa/composer-npm-bridge": true,
      "oat-sa/oatbox-extension-installer": true,
      "composer/package-versions-deprecated": true,
      "openthinklabs/oatbox-extension-installer": true,
      "openthinklabs/composer-npm-bridge": true
    }
  }
}
